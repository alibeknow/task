{
  "name": "unleashed-capital",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "src/gateway/main.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gateway": "nest start gateway --watch",
    "transaction": "nest start transaction --watch",
    "account": "nest start account --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gateway:docker": "node -r ./paths-overwrite.js dist/api/main.js",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down -v",
    "start:rabbit": "docker-compose up -d rabbitmq",
    "typeorm": "ts-node node_modules/typeorm/cli.js -f ormconfig",
    "migration:generate": "npm run typeorm migration:generate -- --name init",
    "migration:create": "npm run typeorm migration:create -d migrations -n init",
    "migration:revert": "npm run typeorm migration:revert",
    "seed:config": "ts-node node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "@types/amqplib": "^0.8.2",
    "@types/compression": "^1.7.2",
    "@types/faker": "^6.6.9",
    "@types/webpack-env": "^1.17.0",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "apollo-server-express": "^3.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "faker": "^5.5.3",
    "graphql": "^16.5.0",
    "helmet": "^5.1.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pino": "^8.1.0",
    "pm": "^2.2.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.2.41",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.2.5",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
